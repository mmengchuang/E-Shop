<script type="text/javascript">
    var vue = new Vue({
        el: "#mymain",
        data: {
            result: [],
        },
        http: {
            headers: {'Content-Type': 'application/x-www-form-urlencoded'}   //1.post请求需设置请求头
        },
        methods: {
            onRegister: function (event) {
                //获取表单参数
                var uname = $("#uname").val();
                var upwd = $("#upwd").val();
                var data = {uname: uname, upwd: upwd}
                //请求数据
                Vue.http.post('/api/users/register', data).then(function (res) {//Vue get 请求
                    if (res.data.error_code == 4000) {
                        window.location.href = "./login"
                    } else {
                        alert(res.data.msg);
                    }
                    //var res_data = JSON.parse(res.data);
                    alert(res_data)
                });
            }
        }
    });
</script>
